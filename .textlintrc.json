{
  "plugins": {},
  "filters": {},
  "rules": {
    "@textlint-rule/textlint-rule-no-unmatched-pair": true,
    "no-todo": true,
    "textlint-rule-common-misspellings": true,
    "textlint-rule-doubled-spaces": true,
    "textlint-rule-en-capitalization": true,
    "textlint-rule-en-spell": true,
    "textlint-rule-ginger": true,
    "textlint-rule-no-start-duplicated-conjunction": true,
    "textlint-rule-rousseau": true,
    "textlint-rule-spellchecker": true,
    "textlint-rule-spelling": true,
    "textlint-rule-terminology": true,
    "textlint-rule-unexpanded-acronym": false,
    "textlint-rule-write-good": true
  }
}
